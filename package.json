{
  "name": "SpotifyJukeboxServer",
  "version": "1.0.0",
  "description": "Jukebox Server",
  "main": "src/index.ts",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimTimTstuff/SpotifyJukeboxServer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TimTimTstuff/SpotifyJukeboxServer/issues"
  },
  "homepage": "https://github.com/TimTimTstuff/SpotifyJukeboxServer#readme",
  "dependencies": {
    "express": "^4.18.2",
    "spotify-web-api-node": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.11",
    "@types/spotify-web-api-node": "^4.0.1",
    "nodemon": "^2.0.20",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
